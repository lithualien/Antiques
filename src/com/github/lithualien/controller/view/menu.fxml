<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.lithualien.controller.Controller">
   <children>
      <TableView fx:id="antiquesTable" layoutX="14.0" layoutY="14.0" prefHeight="500.0" prefWidth="1250.0">
         <columns>
            <TableColumn fx:id="id" editable="false" prefWidth="70.0" resizable="false" text="id" />
            <TableColumn fx:id="name" editable="false" prefWidth="388.0" resizable="false" text="Name" />
            <TableColumn fx:id="condition" editable="false" minWidth="0.0" prefWidth="200.0" resizable="false" text="Condition" />
            <TableColumn fx:id="city" editable="false" prefWidth="200.0" text="Location" />
            <TableColumn fx:id="date" editable="false" prefWidth="200.0" text="Date" />
            <TableColumn fx:id="price" editable="false" prefWidth="200.0" resizable="false" text="Price" />
         </columns>
      </TableView>
      <Button fx:id="showAll" layoutX="22.0" layoutY="551.0" mnemonicParsing="false" onMouseClicked="#getAllAntiques" prefHeight="30.0" prefWidth="125.0" text="Show All" />
      <ChoiceBox fx:id="conditions" layoutX="167.0" layoutY="551.0" prefHeight="30.0" prefWidth="150.0" />
      <Button fx:id="submit" layoutX="496.0" layoutY="551.0" mnemonicParsing="false" onMouseClicked="#getSelectedItems" prefHeight="30.0" prefWidth="125.0" text="Submit" />
      <ChoiceBox fx:id="locations" layoutX="332.0" layoutY="551.0" prefHeight="30.0" prefWidth="150.0" />
      <Button fx:id="sum" layoutX="639.0" layoutY="551.0" mnemonicParsing="false" onMouseClicked="#getPriceSum" prefHeight="30.0" prefWidth="125.0" text="Get Price Sum" />
      <Label fx:id="output" layoutX="22.0" layoutY="614.0" prefHeight="30.0" prefWidth="228.0" />
      <Button fx:id="below" layoutX="786.0" layoutY="551.0" mnemonicParsing="false" onMouseClicked="#getBelowAverage" prefHeight="30.0" prefWidth="125.0" text="Below Average" />
      <Button fx:id="above" layoutX="932.0" layoutY="551.0" mnemonicParsing="false" onMouseClicked="#getAboveAverage" prefHeight="30.0" prefWidth="125.0" text="Above Average" />
   </children>
</AnchorPane>
